<div class="login-page">

  <div class="row">
    <div class="large-12 columns">
	   <% @login_manager = LoginManager.first %>
	   <% unless @login_manager %>
		  <% @login_manager = LoginManager.create() %>
	   <% end %>
	   

      <div class="login-page__header">
        <%= image_tag "logotipo-login.png" %>
        <p>Sistema de Inteligencia y Tecnología Empresarial</p>
      </div><!-- .login-page__header -->

      <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

          <h5>Iniciar sesión</h5>
        
          <div class="field">
            <%= f.label :email %><br />
            <%= f.email_field :email, autofocus: true %>
          </div>

          <div class="field">
            <p>Contraseña</p><br />
            <%= f.password_field :password, autocomplete: "off" %>
          </div>
          <!-- No se pide recordarme
          <% if devise_mapping.rememberable? -%>
            <div class="field">
              <%= f.check_box :remember_me %>
              <%= f.label :remember_me %>
            </div>
          <% end -%>
          -->
          <div class="actions">
            <%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
              <%= link_to "Olvidaste tu contraseña?", new_password_path(resource_name) %><br />
            <% end -%>
            <%= f.submit "Iniciar Sesión" %>
          </div>
        
      <% end %>

