<h1>Usuarios</h1>

<table>
  	<thead>
    	<tr>
            <th>
                Nombre
            </th>
    		<th>
    			Email
    		</th>
    		<th>
    			Rol
    		</th>
    		<th>
    		</th>
    	</tr>
    </thead>
    <tbody>
    	<% @users.each do |user| %>
    		<% unless user.is? 'company' %>
				<tr>
                    <td>
                        <%= user.name %>
                    </td>
					<td>
						<%= user.email %>
					</td>
					<td>
						<%= user.role %>
					</td>
					<td>
                         <%= link_to 'Mostrar', show_user_admin_path(user) %>  
                         <%= link_to 'Editar', edit_user_path(user) %>
                    </td>
				</tr>
			<% end %>
	    <% end %>
    </tbody>
</table>

<%= link_to 'Back', companies_path %>
<%= link_to 'Nuevo Usuario', new_user_registration_path %>